{"name":"20-Waves水波紋","type":"script","author":"ZunkTzckD5EJj8XI","img":"https://assets.forge-vtt.com/bazaar/core/icons/svg/dice-target.svg","scope":"global","command":"// Below, you can turn on the anchor animation.\n\n//把Params置換成想要的特效效果即可\nlet TokenFXFunction = async function () {\nlet params =\n[{\n    filterType: \"wave\",\n    filterId: \"myWaves\",\n    time: 0,\n    anchorX: 0.5,\n    anchorY: 0.5,\n    strength: 0.015,\n    frequency: 120,\n    color: 0xFFFFFF,\n    maxIntensity: 2.5,\n    minIntensity: 0.9,\n    padding:10,\n    animated :\n    {\n      time : \n      { \n        active: true, \n        speed: 0.0085, \n        animType: \"move\" \n      },\n      anchorX :\n      {\n        active: false,\n        val1: 0.15,\n        val2: 0.85,\n        animType: \"syncChaoticOscillation\",\n        loopDuration: 20000\n      },\n      anchorY :\n      {\n        active: false,\n        val1: 0.15,\n        val2: 0.85,\n        animType: \"syncSinOscillation\",\n        loopDuration: 20000\n      }\n    }\n}];\nfor (const token of canvas.tokens.controlled) {\n        if (token.TMFXhasFilterId(params[0][\"filterId\"])) {\n            await token.TMFXdeleteFilters(params[0][\"filterId\"]);\n        } else {\n            await token.TMFXaddUpdateFilters(params);\n        }\n    }\n}\n\n// call the async function\nTokenFXFunction();","folder":null,"sort":0,"permission":{"default":0,"ZunkTzckD5EJj8XI":3},"flags":{"combat-utility-belt":{"macroTrigger":""},"cf":{"id":"temp_bq9pl6wl2x","path":"TOKEN FX","color":"#44d011"}},"_id":"X24ixDRwCePLttjt"}
