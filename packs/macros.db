{"name":"25 - T03 - X-ray Scan (xray) 異界濾鏡","permission":{"default":0,"ZunkTzckD5EJj8XI":3},"type":"script","flags":{"combat-utility-belt":{"macroTrigger":""},"cf":{"id":"temp_5nhghr0amyo","path":"TOKEN FX","color":"#44d011"}},"scope":"global","command":"//把Params置換成想要的特效效果即可\nlet TokenFXFunction = async function () {\nlet params =\n[{\n    filterType: \"xray\",\n    filterId: \"myXrayScan\",\n    time: 0,\n    color: 0xFFFFFF,\n    blend: 5,\n    dimX: 20,\n    dimY: 20,\n    anchorX: 0.5,\n    anchorY: 0,\n    divisor: 8,\n    intensity: 1,\n    animated :\n    {\n       time : \n       { \n          active: true, \n          speed: 0.00038, \n          animType: \"move\" \n       }\n    }\n}];\n for (const token of canvas.tokens.controlled) {\n        if (token.TMFXhasFilterId(params[0][\"filterId\"])) {\n            await token.TMFXdeleteFilters();\n        } else {\n            await token.TMFXaddUpdateFilters(params);\n        }\n    }\n}\n\n// call the async function\nTokenFXFunction();","author":"ZunkTzckD5EJj8XI","img":"icons/svg/dice-target.svg","actorIds":[],"_id":"kUibZCvUFaO08mua"}
